<section class="consumable-section">
	<div class='flexrow items-header'>
		<h3>Consumable</h3>
		<h3 class='item-name'>Name</h3>
		<h3 class='item-formula'>Type</h3>
		<h3>Price</h3>
		<h3 class='item-controls'>
			<a class='item-control item-create' title='{{localize "ASTRO.actor.itemOption.newWeapon"}}' data-type='consumable'
